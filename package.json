  },
  "license": "MIT",
  "keywords": [
    "tailwind",
    "react"
  ],
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "cy:open": "dotenv -- cypress open",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@tailwindcss/postcss7-compat": "2.2.4",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.5",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "autoprefixer": "^9",
    "postcss": "^7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hotkeys-hook": "3.3.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "4.3.5",
    "web-vitals": "2.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.75%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "6.2.0",
    "cypress": "7.7.0",
    "dotenv-cli": "4.0.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,md}": "prettier --write"
  }
}
